
#include <stdio.h>
#include <stdio.h>

char x; 
int main()
{
	printf("Enter the character: ");
	scanf("%c",&x);
	if(x>=65 && x<=90)
	{printf("Captial letter. ");}
	else if(x>=97 && x<=122)
	{printf("Small letter. ");}
	else if(x>=48 && x<=57)
	{printf("Number. ");}
	else
	printf("Special character. ");
}
